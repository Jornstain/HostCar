<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Host CAR</title>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <link href="css/style.css" rel="stylesheet" asp-append-version="true">
    <link href="img/favicon.ico" rel="icon" asp-append-version="true">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com" asp-append-version="true">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700&amp;display=swap" rel="stylesheet" asp-append-version="true">

    <!--FlatPicker -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet" asp-append-version="true">

    <!-- Libraries Stylesheet -->
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" asp-append-version="true">
    <link href="lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" asp-append-version="true">

    <!-- Customized Bootstrap Stylesheet -->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

</head>
<body class="loaded">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-lg position-relative p-0 px-lg-3" style="z-index: 9;">
            <nav class="navbar navbar-expand-lg bg-light navbar-light shadow-lg py-3 py-lg-0 pl-3 pl-lg-5">
                <a href="" class="navbar-brand">
                    <h1 class="m-0 text-primary"><span class="text-dark">Host</span>CAR</h1>
                </a>
                <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-between px-3" id="navbarCollapse">
                    <div class="navbar-nav ml-auto py-0">
                        <a class="nav-link" asp-area="" asp-controller="Rent" asp-action="PreOrderSearch">Rent A Car</a>
                        <div class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle active" data-toggle="dropdown">Cars</a>
                            <div class="dropdown-menu border-0 rounded-0 m-0">
                                <a class="dropdown-item" asp-controller="Car" asp-action="AllCars">All Cars</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-controller="Car" asp-action="SuperCarShow">Super Car</a>
                                <a class="dropdown-item" asp-controller="Car" asp-action="PoorCarShow">Poor Car</a>
                                <a class="dropdown-item" asp-controller="Car" asp-action="MuscleCarShow">Muscle Car</a>
                            </div>
                        </div>
                        <div class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle active" data-toggle="dropdown" style="color:black">Info</a>
                            <div class="dropdown-menu border-0 rounded-0 m-0">
                                <a class="dropdown-item" asp-area="" asp-controller="Info" asp-action="Index">Our Information</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-area="" asp-controller="Info" asp-action="ContactUs">Don't Contact Us</a>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>
        </div>
        <partial name="_LoginPartial" />
    </nav>
    <div class="container-fluid page-header" style="background-color:transparent">
        <div class="container">
            <div class="d-flex flex-column align-items-center justify-content-center" style="min-height: 400px">
                <h3 class="display-4 text-white text-uppercase"></h3>
                <div class="d-inline-flex text-white">
                    <p class="m-0 text-uppercase"><a class="text-white" asp-controller="Rent" asp-action="PreOrderSearch">Home</a></p>
                    <i class="fa fa-angle-double-right pt-1 px-3"></i>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <div class="container-fluid py-5" style="padding-bottom:0px">
        <div class="container pt-5 pb-3">
