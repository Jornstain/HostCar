    <!-- Template Javascript -->
    <script src="~/js/main.js"></script>

    <!-- JavaScript Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" asp-append-version="true"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="lib/easing/easing.min.js" asp-append-version="true"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="lib/tempusdominus/js/moment.min.js" asp-append-version="true"></script>
